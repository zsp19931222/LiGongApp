<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <solid android:color="#FFFFFF" />

            <stroke
                android:width="1dp"
                android:color="#B5B5B5" />
        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:top="1dp">
        <shape>
            <solid android:color="#FFFFFF" />

            <stroke
                android:width="1dp"
                android:color="#ffffff" />
        </shape>
    </item>

</layer-list>